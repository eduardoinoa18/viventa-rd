rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /listing_images/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /user_photos/{userId}/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
